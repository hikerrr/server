{
  "name": "hikerr",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ]
  },
  "scripts": {
    "start": "node ./dist-server/bin/www.js",
    "prod": "npm-run-all build server",
    "server": "node ./dist-server/bin/www.js",
    "dev": "npm-run-all build local",
    "local": "node -r dotenv/config ./dist-server/bin/www.js",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "lint": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "pretty": "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --print-width 80 --write --no-bracket-spacing '**/*.js'",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "json2csv": "^5.0.1",
    "mongoose": "^5.9.15",
    "morgan": "~1.9.1",
    "pug": "^2.0.4",
    "uuidv4": "^6.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
