{
  "name": "hikerr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "dev": "npm-run-all build local",
    "local": "node -r dotenv/config ./dist/bin/www.js",
    "transpile": "babel ./server --out-dir ./dist",
    "clean": "rimraf ./dist",
    "build": "npm-run-all clean transpile",
    "lint": "./node_modules/.bin/eslint --fix ./server",
    "pretty": "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --print-width 80 --write --no-bracket-spacing '**/*.js'",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "aws-sdk": "^2.725.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "connect-mongo": "^3.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.3",
    "json2csv": "^5.0.1",
    "mongoose": "^5.9.27",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^6.4.11",
    "pug": "^2.0.4",
    "uuidv4": "^6.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "babel": {
    "ignore": [
      "node_modules"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  }
}
